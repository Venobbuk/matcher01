        '/(\d+)\s*(?:号|号码|型号|size|sized)/',   // 2号, 2号码, 2型号, 2 size, 2 sized
        '/size\s*(\d+)/',                       // size 2
        '/N\.?(\d+)/'                           // N.2, N2
