        // Year patterns
        '/(\d{4})年?版?/',                      // 2019, 2019年, 2019版
        '/(?:年份|生产年份|出厂年份)\s*(\d{4})/',     // 年份2019, 生产年份2019
        '/(?:year|yr)\.?\s*(\d{4})/',          // year 2019, yr 2019, yr.2019
        '/(\d{4})\s*(?:year|yr)/',              // 2019 year, 2019 yr
        '/(?:vintage|harvest|crop)\s*(\d{4})/',  // vintage 2019, harvest 2019
