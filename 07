        // Model number patterns
        '/[nN][oO]\.?\s*(\d+)/',              // No. 2, No 2, no.2, no 2
        '/[nN](?:um|o|°)(?:ero|\.)?\.?\s*(\d+)/',  // Numero 2, No. 2, N°2
        '/(?:号|号码|型号)\s*(\d+)/',              // 号2, 号码2, 型号2
        '/(\d+)\s*(?:号|号码|型号)/',              // 2号, 2号码, 2型号
       '/No\s*(\d+)/',                        // No 2 (explicit No)
