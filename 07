        // Regional edition patterns
        '/(?:regional|region|regional edition|regional release).*?(\w+)/',  // regional edition asia pacific
        '/(?:区域版|地区版).*?(\w+)/',                                      // 区域版亚太
        '/(\w+).*?(?:regional|region|regional edition|regional release)/',  // asia pacific regional edition
        '/(\w+).*?(?:区域版|地区版)/',                                       // 亚太区域版
        '/(?:exclusivo|exclusive|exc).*?(\w+)/',                          // exclusivo asia pacific
        '/(?:专属|专供|独家).*?(\w+)/',                                    // 专属亚太
        '/(\w+).*?(?:exclusivo|exclusive|exc)/',                          // asia pacific exclusivo
        '/(\w+).*?(?:专属|专供|独家)/',                                     // 亚太专属
        '/(?:for|para).*?(\w+)\s*only/',                                  // for asia pacific only
        '/(?:仅供|仅限).*?(\w+)/',                                          // 仅供亚太
        '/(?:edición regional|er).*?(\w+)/',                              // edición regional asia pacific
        '/(?:er|re)\s*(\w+)/'                                             // er asia pacific, re asia pacific
    ],
